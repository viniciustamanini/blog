<div class="education-fields">
  <%= f.hidden_field :id %>
  <%= f.label :institution_name %>
  <%= f.text_field :institution_name %>

  <%= f.label :degree %>
  <%= f.text_field :degree %>

  <%= f.label :field_of_study %>
  <%= f.text_field :field_of_study %>

  <%= f.label :started_on %>
  <%= f.date_field :started_on %>

  <%= f.label :ended_on %>
  <%= f.date_field :ended_on %>

  <%= f.check_box :_destroy %> <%= f.label :_destroy, 'Remove' %>
</div>

